
class Main {
    public static void main(String[] args) {
        int n=1124;
        int sum=0;
        int product=1;
        int digit=0;
        while(n>0){
            sum=n%10;
            digit+=sum;
            product*=sum;
            n/=10;
        }
        if(digit==product){
            System.out.println("spy number");
        }else{
            System.out.println("not a spy number");
        }
    }
}
