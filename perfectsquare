class Main {
    public static void main(String[] args) {
        int n=9;
        boolean isperfectsquare=false;
        for(int i=0;i<=n;i++){
            if(i*i==n){
                isperfectsquare=true;
                break;
            }
        }
        if(isperfectsquare || n==1){
            System.out.println("perfectsquare");
        }
        else{
            System.out.println("not a perfectsquare");
        }
    }
}
